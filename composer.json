{
  "name": "garret-gunter/doctrine-queue",
  "description": "Queue wrapper for Laravel Doctrine",
  "homepage": "https://github.com/garret-gunter/doctrine-queue",
  "keywords": [
    "laravel",
    "doctrine",
    "queue"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Garret Gunter",
      "email": "garretgun@gmail.com"
    }
  ],
  "type": "library",
  "require": {
    "php": "^7.2",
    "laravel-doctrine/orm": "^1.5",
    "illuminate/queue": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "GarretGunter\\DoctrineQueue\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GarretGunter\\DoctrineQueueTest\\": "tests/"
    }
  },
  "scripts": {
    "test": "php vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "GarretGunter\\DoctrineQueue\\DoctrineQueueProvider"
        ]
    }
  }
}
